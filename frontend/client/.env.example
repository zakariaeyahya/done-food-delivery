# Frontend Client Environment Variables

# Base URL for the backend API
VITE_API_URL="http://localhost:3000/api"

# Smart Contract Addresses (Polygon Mumbai)
# Address of the OrderManager smart contract
VITE_ORDER_MANAGER_ADDRESS="0xYourOrderManagerContractAddress"
# Address of the DONE token smart contract
VITE_TOKEN_ADDRESS="0xYourDoneTokenContractAddress"

# URL for the Socket.io server for real-time updates
VITE_SOCKET_URL="http://localhost:3000"

# Base URL for the IPFS gateway
VITE_IPFS_GATEWAY="https://ipfs.io/ipfs/"

# Google Maps API Key for maps integration (e.g., Autocomplete, Order Tracking Map)
VITE_GOOGLE_MAPS_API_KEY="YourGoogleMapsAPIKey"
